{
  "expo": {
    "name": "Give Me The Aux",
    "slug": "give-me-the-aux",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFB000"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourcompany.givemetheaux",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.yourcompany.givemetheaux"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "givemetheaux",
    "plugins": [
      "expo-dev-client",
      "expo-secure-store",
      "expo-router",
      "expo-notifications"
    ],
    "extra": {
      "eas": {
        "projectId": "4206862e-adc4-4ae3-b2a7-67a8bb9f7fcc"
      }
    }
  }
}
